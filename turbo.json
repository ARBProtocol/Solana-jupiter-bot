{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"outputs": ["dist/**", "build/**"]
		},
		"dev": {
			"cache": false
		},
		"start": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	},
	"globalEnv": ["NODE_ENV", "TUI_INITIAL_SCREEN", "TUI_FPS"]
}
