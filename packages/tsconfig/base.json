{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"skipLibCheck": false,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"isolatedModules": true,
		"incremental": false,
		"noUncheckedIndexedAccess": true,
		"noImplicitAny": true
	},
	"exclude": ["node_modules"]
}
